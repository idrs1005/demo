<%= form_for :register, url: register_create_path do |f| %>
    <p>
      <%= f.label :nombre %><br>
      <%= f.text_field :nombre %>
    </p>

    <p>
      <%= f.label :apellido1 %><br>
      <%= f.text_field :apellido1 %>
    </p>

    <p>
      <%= f.label :apellido2 %><br>
      <%= f.text_field :apellido2 %>
    </p>

    <p>
      <%= f.label :identificacion %><br>
      <%= f.password_field :identificacion %>
    </p>

    <p>
      <%= f.label :email %><br>
      <%= f.text_field :email %>
    </p>

    <p>
      <%= f.label :usuario %><br>
      <%= f.text_field :usuario %>
    </p>

    <p>
      <%= f.label :password %><br>
      <%= f.password_field :password %>
    </p>

    <p>
      <%= f.label :password_confirmation %><br>
      <%= f.password_field :password_confirmation %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>